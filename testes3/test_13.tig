let 

function fib(n:int):int =
let

  var i    := 2
  var Fnew := 1
  var Fold := 0
  var temp := 0
  var ans  := 0

in

  while ( i <= n ) do (
    temp := Fnew;
    Fnew := Fnew + Fold;
    Fold := temp;
    i    := i + 1
  );
  ans := Fnew;
  ans

end

function printint(i: int) =
  let function f(i:int) = if i>0 
             then (f(i/10); print(chr(i-i/10*10+ord("0"))))
   in if i<0 then (print("-"); f(-i))
      else if i>0 then f(i)
      else print("0")
  end

in

  print("Fib: ");printint(fib(30))

end



